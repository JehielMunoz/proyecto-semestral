<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".busquedaProveedor"
    android:background="@color/white">

    <TextView
        android:id="@+id/tituloBienvenida"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="BIENVENIDO"
        android:textColor="@color/btnColor"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="60dp"
        tools:ignore="MissingConstraints"
        />

    <Button
        android:id="@+id/ingresoItemsBtn"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Ingreso ítems"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tituloBienvenida"
        android:layout_marginTop="50dp"
        app:backgroundTint="@color/btnColor"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/buscarProvedorBtn"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Buscar \nProveedor"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ingresoItemsBtn"
        android:layout_marginTop="40dp"
        app:backgroundTint="@color/btnColor"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/registroProvedorBtn"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Registro \nProveedores"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ingresoItemsBtn"
        android:layout_marginTop="40dp"
        app:backgroundTint="@color/btnColor"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/buscarItemsBtn"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Buscar ítems"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tituloBienvenida"
        android:layout_marginTop="50dp"
        app:backgroundTint="@color/btnColor"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/registroUsuariosBtn"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Registro \nUsuarios"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/registroProvedorBtn"
        android:layout_marginTop="40dp"
        app:backgroundTint="@color/btnColor"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/generarActasBtn"
        android:layout_width="150dp"
        android:layout_height="55dp"
        android:text="Generar Actas"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/registroProvedorBtn"
        android:layout_marginTop="40dp"
        app:backgroundTint="@color/btnColor"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/exportarDatosBtn"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Exportar datos"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/registroProvedorBtn"
        android:layout_marginTop="40dp"
        app:backgroundTint="@color/btnColor"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/cerrarSesionBtn"
        android:layout_width="150dp"
        android:layout_height="55dp"
        android:text="Cerrar Sesion"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/generarActasBtn"
        android:layout_marginTop="40dp"
        app:backgroundTint="#B66B3A"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/solicitudAdminBtn"
        android:layout_width="150dp"
        android:layout_height="55dp"
        android:text="Solicitudes"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/generarActasBtn"
        android:layout_marginTop="40dp"
        app:backgroundTint="@color/btnColor"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>