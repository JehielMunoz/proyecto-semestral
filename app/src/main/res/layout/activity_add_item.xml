        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/ubicacionItem"
        app:layout_constraintEnd_toStartOf="@+id/precioItemIVA"
        app:layout_constraintHorizontal_bias="0.884"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:layout_constraintVertical_bias="0.585"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+ IVA TOTAL"
        android:textColor="#009688"

        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/ubicacionItem"
        app:layout_constraintEnd_toStartOf="@+id/precioItemIVA"
        app:layout_constraintHorizontal_bias="0.884"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:layout_constraintVertical_bias="0.585"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"

        android:text="Precio"
        android:textColor="#009688"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/ubicacionItem"

        app:layout_constraintEnd_toStartOf="@+id/precioItem3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/recurso"
        app:layout_constraintVertical_bias="0.342"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"

        android:layout_marginBottom="172dp"
        android:text="Recursos"
        android:textColor="#009688"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/textView11"

        app:layout_constraintEnd_toStartOf="@+id/recurso"
        app:layout_constraintHorizontal_bias="0.217"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fecha Recepcion"
        android:textColor="#009688"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/textView23"
        app:layout_constraintEnd_toStartOf="@+id/fechaRecepcion"
        app:layout_constraintStart_toEndOf="@+id/recurso"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        tools:ignore="MissingConstraints" />

